{
  "extends": [
    "plugin:jsdoc/recommended",
    "plugin:json/recommended",
    "standard"
  ],
  "rules": {
    "jsdoc/newline-after-description": "off"
  },
  "overrides": [
    {
      "files": ["*.jxa"],
      "extends": [
        "jxa"
      ]
    },
    {
      "files": ["Injections/**/*.jxa"],
      "extends": [
        "jxa/objc",
        "jxa/injection"
      ]
    },
    {
      "files": ["Libraries/**/*.jxa"],
      "extends": [
        "jxa/objc",
        "jxa/library"
      ]
    }
  ]
}
